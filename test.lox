fn fib(n) {
  if (n < 2) return n;
  return fib(n -1) + fib(n -2);
}

let N = 40;

let start = clock();
let result = fib(N);
let elapsed = clock() - start;

echo "N: " + N + " Result: " + result  + " Took: " + elapsed + " ms";
echo "N: " + N + " Result: " + result  + " Took: " + elapsed / 1000 + " seconds";

