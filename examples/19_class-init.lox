class User {
  init(name) {
    this.name = name;
  }

  whoami() {
    echo this.name;
  }
}

let user = User("Pepe");
user.whoami();
user.name = "Jordi";
user.whoami();

fn greet() {
  echo "Hello!";
}

user.greet = greet;
user.greet();

user.age = 25;

let u2 = user.init("idk");
echo u2.age;
user.greet();
